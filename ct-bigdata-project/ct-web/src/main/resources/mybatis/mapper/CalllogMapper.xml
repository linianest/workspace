    <select id="queryMonthDatas" resultType="com.ln.ct.web.bean.Calllog">
       select * from ct_call where telid=(
          select
            id
          from ct_user
          where tel=#{tel}
       ) and dateid in (
          select
            id
          from ct_date
          where `year` = #{year} and `month` !='' and `day` is null
       )
    </select>

